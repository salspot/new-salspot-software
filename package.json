{
  "name": "new-salspot-software",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.0.1",
  "scripts": {
    "build": "gatsby build",
    "build-dev": "NODE_ENV=development GATSBY_ENV=development gatsby build --no-uglify",
    "develop": "gatsby develop --host=0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop --host=0.0.0.0",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build && gh-pages -d public -b main"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@loadable/component": "^5.13.1",
    "@mdx-js/react": "^1.6.22",
    "bootstrap": "^5.3.2",
    "emailjs-com": "^3.2.0",
    "gatsby": "^4.21.1",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-mdx": "^3.21.0-next.1",
    "gatsby-plugin-react-helmet": "^5.13.0",
    "gatsby-source-filesystem": "^4.25.0",
    "gatsby-plugin-image": "^2.25.0",
    "gatsby-transformer-sharp": "^4.25.0",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-transformer-remark": "^5.25.1",
    "react": "16.14.0",
    "react-accessible-accordion": "^5.0.0",
    "react-bootstrap": "^2.0.4",
    "react-dom": "^16.9.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.47.0",
    "react-image-lightbox": "^5.1.4",
    "react-modal-video": "^1.2.8",
    "react-owl-carousel3": "^2.2.5",
    "react-scroll-parallax": "^3.1.0",
    "react-toastify": "^9.1.3",
    "react-wow": "^1.0.0",
    "recoil": "^0.7.2"
  },
  "devDependencies": {
    "gh-pages": "^6.0.0",
    "prettier": "2.0.5"
  }
}
