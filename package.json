    "react": "^16.12.0",
    "react-accessible-accordion": "^3.3.3",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-image-lightbox": "^5.1.1",
    "react-modal-video": "^1.2.5",
    "react-owl-carousel3": "^2.2.5",
    "react-scroll-parallax": "^3.1.0",
    "react-wow": "^1.0.0",
    "recoil": "0.0.10"
