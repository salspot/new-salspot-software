{
  "name": "new-salspot-software",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.0.1",
  "scripts": {
    "build": "gatsby build",
    "build-dev": "NODE_ENV=development GATSBY_ENV=development gatsby build --no-uglify",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
